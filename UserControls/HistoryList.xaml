<ItemsControl x:Class="RetailCorrector.Wizard.UserControls.HistoryList"
              xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Width="Auto"
              xmlns:m="clr-namespace:RetailCorrector.Wizard.Managers"             
              ItemsSource="{Binding Source={x:Static m:HistoryController.DisplayItems}}">
    <ItemsControl.ItemsPanel>
        <ItemsPanelTemplate>
            <StackPanel Margin="4"/>
        </ItemsPanelTemplate>
    </ItemsControl.ItemsPanel>
</ItemsControl>
