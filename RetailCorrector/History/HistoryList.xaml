<ItemsControl x:Class="RetailCorrector.History.HistoryList"
              xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Width="Auto"
              xmlns:l="clr-namespace:RetailCorrector.History"             
              ItemsSource="{Binding Source={x:Static l:HistoryController.DisplayItems}}">
    <ItemsControl.ItemsPanel>
        <ItemsPanelTemplate>
            <StackPanel Margin="4"/>
        </ItemsPanelTemplate>
    </ItemsControl.ItemsPanel>
</ItemsControl>
